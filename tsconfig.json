{
  "compilerOptions": {
    "target": "ES5",
    "module": "ESNext",
    "allowJs": true,
    "sourceMap": true,
    "outDir": "./dist/",
    "downlevelIteration": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "lib":[
      "DOM",
      "ESNext"
    ],
    "jsx": "react-jsx",
    "noEmit": true,
    "isolatedModules": true,
    "strict": true,
    "resolveJsonModule": true
  },
  "include": ["src/**/*"]
}